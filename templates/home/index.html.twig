<table style="width:100%">
    <tr>
        <th>Title</th>
        <th>Authors</th>
        <th>Categories</th>
        <th>Price</th>
    </tr>
    {% for book in books %}
        <tr>
            <td>{{ book.title }}</td>
            {% for author in book.author %}
                <td>{{ author.name }}</td>
            {% endfor %}
            {% for category in book.category %}
                <td>{{ category.type }}</td>
            {% endfor %}
            <td>{{ book.price }}</td>
        </tr>
    {% endfor %}
</table>
